      <Query>
        <DataSourceName>DS_RLS</DataSourceName>
        <CommandText>SELECT DISTINCT li.Department, li.ApplicationTypeID, li.GradeID, li.IssueDate, li.LoanStatusID, li.LoanAmount, li.TotalPayment
